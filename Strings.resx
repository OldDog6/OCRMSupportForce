  <data name="IndBasicQ" xml:space="preserve">
    <value>SELECT sum(causeview__Amount__c),
max(causeview__Date__c),
causeview__Donation__r.causeview__Constituent__r.Internal_Solicitor__c,  
causeview__Donation__r.causeview__Constituent__r.causeview__primary_Addressee__c,
causeview__Donation__r.causeview__Constituent__r.mailingStreet,
causeview__Donation__r.causeview__Constituent__r.mailingCity,
causeview__Donation__r.causeview__Constituent__r.mailingPostalCode,
causeview__Donation__r.causeview__Constituent__r.mailingState,
causeview__Donation__r.causeview__Constituent__r.Email,
causeview__Donation__r.causeview__Constituent__r.Phone,
causeview__Donation__r.causeview__Gift_Type__c 
FROM causeview__Payment__c
where causeview__Date__c &gt; 2013-12-31
and causeview__Date__c &lt; 2015-01-01
and causeview__Donation__r.causeview__Constituent__c &lt;&gt; null
and causeview__amount__c &gt; 999
and causeview__Donation__r.causeview__Gift_Type__c &lt;&gt; 'Gift in Kind'
group by causeview__Donation__r.causeview__Constituent__c,
causeview__Donation__r.causeview__Gift_Type__c,
causeview__Donation__r.causeview__Constituent__r.causeview__primary_Addressee__c,
causeview__Donation__r.causeview__Constituent__r.mailingStreet,
causeview__Donation__r.causeview__Constituent__r.mailingCity,
causeview__Donation__r.causeview__Constituent__r.mailingPostalCode,
causeview__Donation__r.causeview__Constituent__r.mailingState,
causeview__Donation__r.causeview__Constituent__r.Email,
causeview__Donation__r.causeview__Constituent__r.Phone,
causeview__Donation__r.causeview__Constituent__r.Internal_Solicitor__c
order by sum(causeview__amount__c) desc 
LIMIT 1000</value>
  </data>
